  "scripts": {
    "dev:auth": "pnpm --filter @apps/auth dev",
    "dev:api": "pnpm --filter @apps/api dev",
    "dev:app": "pnpm --filter @apps/app dev",
    "dev": "pnpm build && NODE_TLS_REJECT_UNAUTHORIZED=0 concurrently -n auth,api,app -c blue,green,magenta \"pnpm dev:auth\" \"pnpm dev:api\" \"pnpm dev:app\"",
